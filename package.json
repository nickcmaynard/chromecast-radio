{
  "name": "chromecast-radio",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "dev-frontend": "ng serve --host 0.0.0.0 --proxy-config proxy.conf.json --verbose",
    "build": "ng build",
    "//": "replace the WEBAPP_TITLE with that in .env",
    "postbuild": "dotenv npm run replace-dist-title",
    "replace-dist-title": "replace 'WEBAPP_TITLE' \"${WEBAPP_TITLE:-WEBAPP_TITLE}\" dist/index.html",
    "test": "ng test",
    "lint": "ng lint",
    "start": "node server",
    "start-forever": "forever start --uid 'cc-radio' -sa --minUptime 1000 --spinSleepTime 1000 server",
    "restart-forever": "forever restart -s 'cc-radio'",
    "stop-forever": "forever stop -s server"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.1.0",
    "@angular/cdk": "^14.1.0",
    "@angular/common": "^14.1.0",
    "@angular/compiler": "^14.1.0",
    "@angular/core": "^14.1.0",
    "@angular/forms": "^14.1.0",
    "@angular/material": "^14.1.0",
    "@angular/platform-browser": "^14.1.0",
    "@angular/platform-browser-dynamic": "^14.1.0",
    "@angular/router": "^14.1.0",
    "bent": "^7.3.12",
    "body-parser": "^1.18.2",
    "castv2-client": "^1.2.0",
    "castv2-device-monitor": "^1.3.2",
    "config": "^1.29.1",
    "core-js": "^2.4.1",
    "debounce": "^1.2.0",
    "debug": "^3.1.0",
    "domhandler": "^5.0.3",
    "domutils": "^3.0.1",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "forever": "^3.0.0",
    "hammerjs": "^2.0.8",
    "htmlparser2": "^8.0.1",
    "jsonp-client": "^1.1.1",
    "mdns-js": "^1.0.1",
    "ngx-socket-io": "^4.3.0",
    "ngx-swiper-wrapper": "^10.0.0",
    "object-hash": "^3.0.0",
    "q": "^1.5.1",
    "rxjs": "^7.5.0",
    "socket.io": "^4.5.1",
    "tslib": "^2.3.0",
    "typeface-overpass": "0.0.54",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.1.0",
    "@angular/cli": "~14.1.0",
    "@angular/compiler-cli": "^14.1.0",
    "@angular/language-service": "^14.1.0",
    "@types/jasmine": "~4.0.0",
    "@types/node": "^6.0.96",
    "codelyzer": "^6.0.2",
    "dotenv-cli": "^1.4.0",
    "jasmine-core": "~4.2.0",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "replace": "^1.0.0",
    "swiper": "^6.4.15",
    "ts-node": "~3.2.0",
    "tslint": "~5.7.0",
    "typescript": "~4.7.2"
  }
}
